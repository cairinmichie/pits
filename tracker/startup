#!/bin/bash
cd /home/pi/pits/tracker
sudo modprobe w1-gpio
sudo modprobe w1_therm
sudo modprobe i2c-dev
echo none | sudo tee /sys/class/leds/led0/trigger
echo 1 | sudo tee /sys/class/leds/led0/brightness
#python3 sensors.py &
#python3 astro_led.py &
#python3 webstuff.py
while :
do
	sudo ./tracker
	sleep 5
done
